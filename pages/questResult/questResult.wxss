.section{
    padding-bottom: 100rpx;
}

.ongingPost{
    position: relative;
    padding: 20rpx 20rpx 20rpx 285rpx;
    height: 185rpx;
    border-bottom: 1rpx solid #e9e9e9;
}

.ongingPost .userinfo-avatar {
  width: 150rpx;
  height: 150rpx;
  top: 10 rpx;
  border-radius: 50%;
  position: absolute;
  left: 20rpx;
}

.ongingPost .userinfo-nickname{
    position: absolute;
    left: 20rpx;
    bottom: 20rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 50rpx;
    overflow: hidden;
    text-align: center;
    font-size: 44rpx;
}